{
  "parser": "babel-eslint",
    "rules": {
      "strict" : 2,
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": [
          0
        ]
    },
    "globals": {
      "console": false,
      "describe": false,
      "it": false,
      "expect": false,
      "fail": false,
      "module": false,
      "jasmine": false,
      "beforeEach": false,
      "afterEach": false
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
  "extends": "eslint:recommended"
}
